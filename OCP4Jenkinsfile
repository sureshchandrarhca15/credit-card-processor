node {
     stage('Build') 
	 {
        try 
		{
		    container('maven')
			{
            sh """
            mvn clean install -Dmaven.test.skip=true
            """
            }
      }
      catch (Exception e) 
	  {
        println "Failed to Maven Build - ${currentBuild.fullDisplayName}"
        throw e
      }
    }
}