def version, mvnCmd = "mvn -s templates/cicd-settings-nexus3.xml"
pipeline
{
  agent
  {
    label 'maven'
  }

    stages
    {
      stage('Build App')
      {
        steps
        {
			checkout([$class: 'GitSCM', branches: [[name: '*/master']], [[url: 'https://github.com/sureshchandrarhca15/credit-card-processor.git']]])
            script 
			{
             echo "Checkout Done"    
            }
             sh "mvn install -DskipTests=true"
            }
      }
	}
}
